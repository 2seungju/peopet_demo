  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "sitemap": "node './config/buildSitemap.js'",
    "export": "&& next export && cp -R ./.next/static ./out/_next/static/",
    "start": "NODE_ENV=production node server.js",
